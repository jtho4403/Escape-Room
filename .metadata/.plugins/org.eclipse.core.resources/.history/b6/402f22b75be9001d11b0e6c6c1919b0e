//	uint8_t *seq[8];
//	Sequence(seq, count);

	DeclareSequence();

	switch(count){
	case 1: {
		if (input == seq1){
			*flag = 1;
		}

	}
	case 2: {
		if (input == seq1){
			*flag = 1;
